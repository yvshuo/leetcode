{
  "extends": "airbnb/legacy",
  "rules": {
    "no-unused-vars": 0,
    "func-names": 0,
    "no-console": 0,
    "vars-on-top": 0,
    "no-use-before-define": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-param-reassign": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "spread": true,
      "restParams": true
    }
  },
  "parser": "babel-eslint",
  "env": {
    "mocha": true
  }
}
